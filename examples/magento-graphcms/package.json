{
  "name": "@graphcommerce/magento-graphcms",
  "version": "2.138.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "yarn concurrently -k -n codegen,next 'yarn codegen-base -w' 'yarn next dev'",
    "codegen": "yarn mesh build --require dotenv/config && NEXT_PUBLIC_GRAPHQL_ENDPOINT=.mesh/schema.graphql yarn codegen-base",
    "build": "yarn codegen && yarn next build && yarn next-sitemap",
    "start": "next start",
    "tsc:lint": "yarn tsc --noEmit -p .",
    "lingui": "NODE_ENV=development lingui extract",
    "codegen-base": "NODE_TLS_REJECT_UNAUTHORIZED=0 CHOKIDAR_USEPOLLING=0 yarn graphql-codegen --require dotenv/config -c codegen.mono.yml",
    "prepack": "concurrently 'yarn prepack:1' 'yarn prepack:2' 'yarn prepack:3'",
    "prepack:1": "yarn workspace @graphcommerce/next-config prepack",
    "prepack:2": "yarn workspace @graphcommerce/graphql-codegen-near-operation-file prepack",
    "prepack:3": "yarn workspace @graphcommerce/graphql-codegen-relay-optimizer-plugin prepack",
    "postinstall": "patch-typed-document-node && yarn prepack"
  },
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.0.2",
    "@apollo/client": "^3.4.16",
    "@graphcommerce/browserslist-config-pwa": "^3.0.2",
    "@graphcommerce/eslint-config-pwa": "^3.1.5",
    "@graphcommerce/framer-next-pages": "^2.107.5",
    "@graphcommerce/googlerecaptcha": "^1.1.3",
    "@graphcommerce/googletagmanager": "^1.0.3",
    "@graphcommerce/graphcms-ui": "^2.106.0",
    "@graphcommerce/graphql": "^2.105.5",
    "@graphcommerce/graphql-codegen-near-operation-file": "^2.103.13",
    "@graphcommerce/graphql-codegen-relay-optimizer-plugin": "^2.102.13",
    "@graphcommerce/graphql-mesh": "^3.2.5",
    "@graphcommerce/image": "^2.105.4",
    "@graphcommerce/lingui-next": "^1.2.0",
    "@graphcommerce/magento-cart": "^3.8.0",
    "@graphcommerce/magento-cart-billing-address": "^2.107.5",
    "@graphcommerce/magento-cart-checkout": "^2.107.5",
    "@graphcommerce/magento-cart-coupon": "^2.109.0",
    "@graphcommerce/magento-cart-email": "^2.107.0",
    "@graphcommerce/magento-cart-items": "^2.110.5",
    "@graphcommerce/magento-cart-payment-method": "^2.111.5",
    "@graphcommerce/magento-cart-shipping-address": "^2.106.5",
    "@graphcommerce/magento-cart-shipping-method": "^2.106.5",
    "@graphcommerce/magento-category": "^3.5.0",
    "@graphcommerce/magento-cms": "^3.2.5",
    "@graphcommerce/magento-customer": "^3.6.5",
    "@graphcommerce/magento-customer-account": "^2.105.5",
    "@graphcommerce/magento-customer-order": "^2.105.5",
    "@graphcommerce/magento-graphql": "^2.104.5",
    "@graphcommerce/magento-newsletter": "^1.1.5",
    "@graphcommerce/magento-payment-braintree": "^2.106.5",
    "@graphcommerce/magento-payment-included": "^2.105.5",
    "@graphcommerce/magento-product": "^3.7.0",
    "@graphcommerce/magento-product-bundle": "^3.2.5",
    "@graphcommerce/magento-product-configurable": "^3.6.0",
    "@graphcommerce/magento-product-downloadable": "^3.2.5",
    "@graphcommerce/magento-product-grouped": "^2.105.5",
