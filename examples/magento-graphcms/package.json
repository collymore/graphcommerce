    "@playwright/test": "^1.16.2",
    "apollo3-cache-persist": "^0.13.0",
    "cheerio": "^1.0.0-rc.10",
    "clsx": "^1.1.1",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "html-react-parser": "^1.4.0",
    "next": "^12.0.3",
    "next-pwa": "^5.4.0",
    "next-sitemap": "^1.6.203",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "schema-dts": "^1.0.0",
    "sharp": "^0.29.2",
    "vercel": "^23.1.2"
  },
  "devDependencies": {
    "@lingui/cli": "^3.12.1",
    "eslint": "^8.2.0",
    "type-fest": "^2.5.1",
    "typescript": "^4.4.4"
  },
  "prettier": "@graphcommerce/prettier-config-pwa",
  "browserslist": [
    "extends @graphcommerce/browserslist-config-pwa"
  ],
  "eslintConfig": {
    "extends": "@graphcommerce/eslint-config-pwa",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}
